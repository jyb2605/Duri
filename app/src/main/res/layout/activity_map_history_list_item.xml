<?xml version="1.0" encoding="utf-8"?><RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"    xmlns:app="http://schemas.android.com/apk/res-auto"    xmlns:tools="http://schemas.android.com/tools"    android:id="@+id/activity_main"    android:layout_width="match_parent"    android:layout_height="wrap_content"    android:background="#fdded8"    android:orientation="vertical"    android:weightSum="1"    tools:context=".RecordActivity">    <FrameLayout        android:layout_width="match_parent"        android:layout_height="200dp"        android:layout_alignParentStart="true"        android:layout_alignParentTop="true"        android:background="@color/colorPrimary">        <RelativeLayout            android:layout_width="match_parent"            android:layout_height="70dp"            android:layout_gravity="bottom"            android:orientation="horizontal">            <TextView                android:id="@+id/textView4"                android:layout_width="wrap_content"                android:layout_height="wrap_content"                android:layout_alignParentStart="true"                android:layout_alignParentTop="true"                android:layout_marginStart="45dp"                android:layout_marginTop="12dp"                android:text="2/13"                android:textSize="20sp" />            <TextView                android:id="@+id/textView5"                android:layout_width="wrap_content"                android:layout_height="wrap_content"                android:layout_alignStart="@+id/textView4"                android:layout_below="@+id/textView4"                android:text="아침산책" />            <TextView                android:id="@+id/textView6"                android:layout_width="wrap_content"                android:layout_height="wrap_content"                android:layout_alignParentEnd="true"                android:layout_centerVertical="true"                android:layout_marginEnd="17dp"                android:text="2,420 걸음" />        </RelativeLayout>    </FrameLayout></RelativeLayout>