<?xml version="1.0" encoding="utf-8"?><RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"    xmlns:app="http://schemas.android.com/apk/res-auto"    android:layout_width="match_parent" android:layout_height="match_parent">    <RelativeLayout        android:id="@+id/music_btn"        android:layout_width="fill_parent"        android:layout_height="160dp"        android:orientation="vertical"        android:layout_above="@+id/walk_btn"        android:layout_alignParentStart="true">        <Button            android:id="@+id/inner"            android:layout_width="84dp"            android:layout_height="64dp"            android:layout_marginBottom="5dp"            android:layout_gravity="center_horizontal"            android:background="@drawable/btn_music_selector"            android:layout_centerHorizontal="true" />        <TextView            android:id="@+id/textView3"            android:layout_width="80dp"            android:layout_height="wrap_content"            android:text="노래 틀어주기"            android:layout_below="@+id/inner"            android:layout_alignStart="@+id/inner" />    </RelativeLayout>    <RelativeLayout        android:id="@+id/walk_btn"        android:layout_width="160dp"        android:layout_height="150dp"        android:layout_marginLeft="50px"        android:layout_marginRight="25px"        android:layout_marginStart="27dp"        android:layout_alignTop="@+id/record_btn"        android:layout_alignParentStart="true">        <TextView            android:id="@+id/textView4"            android:layout_width="wrap_content"            android:layout_height="wrap_content"            android:text="산책하기"            android:layout_below="@+id/inner2"            android:layout_centerHorizontal="true"            android:layout_marginTop="10dp" />        <Button            android:id="@+id/inner2"            android:layout_width="69dp"            android:layout_height="64dp"            android:layout_marginTop="17dp"            android:layout_gravity="center_horizontal"            android:background="@drawable/btn_walk_selector"            android:layout_alignParentTop="true"            android:layout_centerHorizontal="true" />    </RelativeLayout>    <RelativeLayout        android:id="@+id/record_btn"        android:layout_width="160dp"        android:layout_height="150dp"        android:layout_marginLeft="50px"        android:layout_marginRight="25px"        android:layout_above="@+id/music_cnt"        android:layout_alignParentEnd="true"        android:layout_marginEnd="10dp">        <TextView            android:id="@+id/textView5"            android:layout_width="wrap_content"            android:layout_height="wrap_content"            android:text="녹음하기"            android:layout_marginTop="17dp"            android:layout_below="@+id/inner3"            android:layout_centerHorizontal="true" />        <Button            android:id="@+id/inner3"            android:layout_width="34dp"            android:layout_height="56dp"            android:layout_marginTop="17dp"            android:layout_gravity="center_horizontal"            android:background="@drawable/btn_record_selector"            android:layout_alignParentTop="true"            android:layout_centerHorizontal="true" />    </RelativeLayout>    <RelativeLayout        android:id="@+id/music_cnt"        android:layout_width="fill_parent"        android:layout_height="50dp"        android:layout_alignParentBottom="true"        android:background="#81b530"        >        <TextView            android:id="@+id/musicname_txt"            android:layout_width="wrap_content"            android:layout_height="wrap_content"            android:layout_marginTop="15dp"            android:layout_marginLeft="30dp"            android:layout_alignParentLeft="true"            android:text="노래제목이다"            android:textColor="#FFFFFF"/>        <Button            android:id="@+id/onoff_btn"            android:layout_width="28dp"            android:layout_height="28dp"            android:layout_alignParentRight="true"            android:layout_marginRight="30dp"            android:layout_marginTop="12dp"            android:background="@drawable/btn_pause_selector"            />    </RelativeLayout>    <RelativeLayout        android:id="@+id/sub_layout"        android:layout_width="match_parent"        android:layout_height="200dp"        android:layout_alignParentTop="true"        android:layout_alignParentStart="true">        <TextView            android:id="@+id/textView6"            android:layout_width="wrap_content"            android:layout_height="wrap_content"            android:text="(남,4세)"            android:layout_below="@+id/petname_txt"            android:layout_centerHorizontal="true" />        <ImageButton            android:id="@+id/option_btn"            android:layout_width="20dp"            android:layout_height="20dp"            android:layout_marginRight="20dp"            android:layout_marginTop="20dp"            android:layout_alignParentRight="true"            android:layout_alignParentTop="true"            android:background="@drawable/btn_settings_selector"            />        <TextView            android:id="@+id/petname_txt"            android:layout_width="wrap_content"            android:layout_height="wrap_content"            android:text="몽이"            android:layout_below="@+id/pet_img"            android:layout_centerHorizontal="true" />        <ImageView            android:id="@+id/pet_img"            android:layout_width="90dp"            android:layout_height="90dp"            android:background="@drawable/picture"            android:layout_below="@+id/option_btn"            android:layout_centerHorizontal="true" />        <TextView            android:id="@+id/textView7"            android:layout_width="wrap_content"            android:layout_height="wrap_content"            android:text="2,367"            android:layout_alignParentTop="true"            android:layout_alignStart="@+id/petname_txt"            android:layout_marginTop="18dp"/>        <ImageView            android:id="@+id/imageView5"            android:layout_width="wrap_content"            android:layout_height="wrap_content"            app:srcCompat="@drawable/icon_walk"            android:layout_alignBottom="@+id/textView7"            android:layout_alignStart="@+id/pet_img"/>    </RelativeLayout></RelativeLayout>